{
  "name": "playlister",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "yarn prettier --check --ignore-unknown .",
    "format:write": "yarn prettier --write --ignore-unknown .",
    "generate:component": "npx generate-react-cli component",
    "generate:theme": "chakra-cli tokens src/theme/index.ts",
    "postinstall": "yarn generate:theme",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/query-sync-storage-persister": "4.35.0",
    "@tanstack/react-query": "4.35.0",
    "@tanstack/react-query-devtools": "4.35.0",
    "@tanstack/react-query-persist-client": "4.35.0",
    "@tanstack/react-table": "^8.10.0",
    "axios": "^1.5.0",
    "framer-motion": "^10.16.4",
    "next": "13.4.13",
    "next-auth": "^4.23.1",
    "next-seo": "^6.1.0",
    "next-translate": "^2.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.19",
    "@storybook/react": "^7.4.3",
    "@tanstack/eslint-plugin-query": "4.34.1",
    "@types/node": "^17.0.18",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/spotify-api": "^0.0.22",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "generate-react-cli": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "next-translate-plugin": "^2.5.3",
    "prettier": "3.0.3",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --ignore-unknown --write"
    ]
  },
  "private": true
}
